{
    "rules": {
        "indent": [
            2,
            4
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-underscore-dangle": 0,
        "curly": [
            2,
            "multi-line"
        ],
        "space-infix-ops": [
            2,
            {
                "int32Hint": true
            }
        ],
        "no-empty": 0,
        "no-unused-vars": 0
    },
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "node": true,
        "browser": true
    },
    "extends": "eslint:recommended"
}
